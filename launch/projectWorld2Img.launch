<!--start a bagfile-->
<launch>

<param name="use_sim_time" type="bool" value="True"/>
<include file="$(find launch)/config/server.launch" />

<!-- start bagfile -->
<node name="rosbag" pkg="rosbag" type="play" args="--clock /home/yvonne/bags/center_of_field{7}.bag" output="screen">
  </node>
  
 <node pkg="image_view" name="view_image" type="image_view" >
         <remap from="image" to="/vision/takenImg"/>
   </node > 

  <!-- start to project world points to image plane -->
<node name="world2Img" pkg="vision_6d_pose" type="projectWorld2Img" output="screen">
  </node>
  
  
 <node pkg="rviz" name = "rviz1" type = "rviz"   args="-d $(find vision_6d_pose)/config/vision.rviz">
    </node>

  <node pkg="image_view" name="view_image2" type="image_view" >
         <remap from="image" to="/projected_image"/>
   </node > 
   
   
  <!--node name="extract" pkg="image_view" type="extract_images" respawn="false" output="screen" cwd="ROS_HOME">
      <remap from="image" to="/projected_image"/>
  </node-->
    
</launch>
